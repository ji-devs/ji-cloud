    "node-fetch": "^2.7.0",
    "puppeteer": "^22.0.0",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.5.1",
    "cross-env": "^7.0.2"
